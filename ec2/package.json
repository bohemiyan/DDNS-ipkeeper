{
  "name": "ipkeeper-ec2",
  "version": "1.0.0",
  "description": "IPKeeper EC2 Reverse Proxy Server - Dynamic IP management for home servers using AWS free tier",
  "main": "index.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "dev": "nodemon index.js",
    "pm2": "pm2 start index.js --name ipkeeper-ec2",
    "pm2:stop": "pm2 stop ipkeeper-ec2",
    "pm2:restart": "pm2 restart ipkeeper-ec2",
    "pm2:logs": "pm2 logs ipkeeper-ec2",
    "pm2:delete": "pm2 delete ipkeeper-ec2"
  },
  "keywords": [
    "dynamic-ip",
    "reverse-proxy",
    "aws-ec2",
    "home-server",
    "ddns",
    "free-tier",
    "proxy"
  ],
  "author": "IPKeeper Contributors",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/bohemiyan/ipkeeper.git"
  },
  "homepage": "https://github.com/bohemiyan/ipkeeper#readme",
  "bugs": {
    "url": "https://github.com/bohemiyan/ipkeeper/issues"
  },
  "engines": {
    "node": ">=16.0.0",
    "npm": ">=7.0.0"
  },
  "dependencies": {
    "dotenv": "^16.5.0",
    "express": "^5.1.0",
    "express-rate-limit": "^7.5.1",
    "helmet": "^8.1.0",
    "http": "^0.0.1-security",
    "http-proxy": "^1.18.1",
    "morgan": "^1.10.0",
    "net": "^1.0.2",
    "nodemon": "^3.1.10",
    "pm2": "^6.0.8",
    "validator": "^13.15.15"
  }
}